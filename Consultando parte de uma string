

use dbFuncionario;

SELECT nome
FROM funcionario
WHERE nome LIKE 'A%';

SELECT nome
FROM funcionario
WHERE nome LIKE '%Costa%';

SELECT nome
FROM funcionario
WHERE nome LIKE 'Barbosa%';

SELECT nome
FROM funcionario
WHERE nome LIKE '%Sou_a%';

SELECT nome
FROM funcionario
WHERE nome NOT LIKE 'Ana%';

SELECT nome, salario
FROM funcionario
WHERE nome NOT LIKE 'Ana%' and salario between 1000 and 2000;
order by salario desc

use dbpaciente; 

select nome, ocorrencia
from paciente 
where ocorrencia like '%c_lica%' and idade >=18
order by idade ASC; 
