select * from dependente 

inner join funcionario

on dependente.cod_func = funcionario.id_func
where status_dep = 'filho';


select nome_func from dependente

inner join funcionario

on dependente.cod_func = funcionario.id_func
where status_dep = 'conjuge';


select nome_func,nome_dep from dependente

inner join funcionario

on dependente.cod_func = funcionario.id_func
where status_dep = 'esposa';



select * from dependente

inner join funcionario

on dependente.cod_func = funcionario.id_func
inner join localidade
on localidade.id_local = funcionario.id_localidade;

