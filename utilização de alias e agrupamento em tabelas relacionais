
select id_func,count(*)   
from funcionario
inner join dependente

on funcionario.id_func = dependente.cod_func
group by id_func;


select id_func,count(*)

from funcionario

inner join dependente

on funcionario.id_func = dependente.cod_func

group by id_func;





select nome_func,count(*)

from funcionario

inner join dependente

on funcionario.id_func = dependente.cod_func

group by nome_func;




select nome_func as 'Nome do Funcionário',count(*) as 'Quantidade de Dependentes'

from funcionario

inner join dependente

on funcionario.id_func = dependente.cod_func

group by nome_func;



select nome_func as 'Nome do Funcionário',count(*) as 'Quantidade de Dependentes'

from funcionario
inner join dependente

on funcionario.id_func = dependente.cod_func

group by nome_func
order by 2;





















