select * from curso_teste;



select nome_curso AS 'Nome do Curso' 

from curso_teste;



select nome_curso as Curso, ch_curso as 'Carga Horária'

from curso_teste;



select nome_curso as 'Nome do Curso' 

from curso_teste

order by nome_curso;



select nome_curso as 'Nome do Curso' 

from curso_teste
order by 1;



select nome_curso as Curso, ch_curso as 'Carga Horária'
from curso_teste
order by 2 ASC;



select nome_curso as Curso, ch_curso as 'Carga Horária' 
from curso_teste

order by ch_curso ASC;



select nome_curso as Curso, ch_curso as 'Carga Horária'

from curso_teste
order by nome_curso;




select * from curso_teste;



select cat_curso,nome_curso
from curso_teste;



select cat_curso,nome_curso

from curso_teste

order by 1, 2;



select cat_curso,nome_curso

from curso_teste
order by 1, 2 desc;



select * from curso_teste;



select ch_curso As 'Carga Horária', nome_curso as Curso

from curso_teste

order by ch_curso desc;


