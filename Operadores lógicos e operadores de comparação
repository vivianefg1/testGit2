select * from curso_teste

where ch_curso <=10;


select * from curso_teste

where ch_curso <=5 OR ch_curso >=10;



select cat_curso,nome_curso,ch_curso

from curso_teste

where ch_curso <=5 AND cat_curso = 'tecnologia';




select * from curso_teste

where cat_curso ='tecnologia' or cat_curso='idiomas';



select * from curso_teste

where cat_curso ='tecnologia' or cat_curso='idiomas'

order by cat_curso;



select * from curso_teste

where cat_curso not in ('tecnologia','idiomas');
















